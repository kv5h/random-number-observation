{
    "compilerOptions": {
        "target": "es2020",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./.netlify/functions-build",
        "rootDir": "./netlify/functions",
        "resolveJsonModule": true
    },
    "include": [
        "netlify/functions/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}
